<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>search</servlet-name>
  	<display-name>search</display-name>
  	<description></description>
  	<servlet-class>rpc.SearchItem</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>history</servlet-name>
  	<display-name>history</display-name>
  	<description></description>
  	<servlet-class>rpc.ItemHistory</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>recommendation</servlet-name>
  	<display-name>recommendation</display-name>
  	<description></description>
  	<servlet-class>rpc.RecommendItem</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>login</servlet-name>
  	<display-name>login</display-name>
  	<description></description>
  	<servlet-class>rpc.Login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Logout</servlet-name>
  	<display-name>Logout</display-name>
  	<description></description>
  	<servlet-class>rpc.Logout</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>register</servlet-name>
  	<display-name>register</display-name>
  	<description></description>
  	<servlet-class>rpc.Register</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>search</servlet-name>
  	<url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>history</servlet-name>
  	<url-pattern>/history</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>recommendation</servlet-name>
  	<url-pattern>/recommendation</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Logout</servlet-name>
  	<url-pattern>/Logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>register</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
</web-app>
